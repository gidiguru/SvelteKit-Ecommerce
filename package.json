{
	"name": "svelte-ecom",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "node build/output/server/index.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx src/lib/server/db/seed.ts",
		"db:drop": "tsx drizzle-kit drop",
		"email": "tsx src/lib/emails/resend-test.ts",
		"stripe:listen": "stripe listen --forward-to http://localhost:5173/api/stripe",
		"email:dev": "email dev --dir src/lib/emails"
	},
